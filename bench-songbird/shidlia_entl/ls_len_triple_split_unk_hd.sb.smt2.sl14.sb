
data Refnode {
    Refnode next;
};

pred ls(x_1,y_2,n_3) := 
    (emp & n_3=0 & x_1=y_2)
    \/ (exists u_4,k. x_1->Refnode{next:u_4} * ls(u_4,y_2,k) & k=n_3-1 & 0<=n_3-1);


checkentail ls(x,y,k1000) & k1000=1000 & k800=800 & k10=10
    |- (exists u,v,m. ls(u,v,k10) * ls(v,y,k800) * ls(x,u,m) ) ;
