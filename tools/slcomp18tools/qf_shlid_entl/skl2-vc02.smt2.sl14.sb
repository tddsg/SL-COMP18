
data RefSL2_t {
    RefSL2_t n1;
    RefSL2_t n2;
};

pred skl1(hd,ex) := 
    (emp & hd=ex)
    \/ (exists tl. hd->RefSL2_t{n1:tl,n2:nil} * skl1(tl,ex) & hd!=ex);

pred skl2(hd,ex) := 
    (emp & hd=ex)
    \/ (exists tl,Z1. hd->RefSL2_t{n1:Z1,n2:tl} * skl1(Z1,tl) * skl2(tl,ex) & hd!=ex);


checkentail x1->RefSL2_t{n1:x1_1,n2:x2} * x1_1->RefSL2_t{n1:x2,n2:nil} * x2->RefSL2_t{n1:nil,n2:nil}
    |- skl2(x1,nil);
