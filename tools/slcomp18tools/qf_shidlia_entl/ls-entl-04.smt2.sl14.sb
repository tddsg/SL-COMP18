
data RefSll_t {
    RefSll_t next;
    unknown data_sbd;
};

pred sls(in,dt1,out,dt2) := 
    (emp & in=out & dt1=dt2)
    \/ (exists u,dt3. in->RefSll_t{next:u,data_sbd:dt1} * sls(u,dt3,out,dt2) & error);


checkentail sls(X,a,Y0,b0) * Y0->RefSll_t{next:Y1,data_sbd:b0} * sls(Y1,b1,Z,c) & error & error & error
    |- sls(X,a,Z,c) & error;
