
data RefSll_t {
    RefSll_t next;
    unknown data_sbd;
};

pred lsls(in,dt1,len1,out,dt2,len2) := 
    (emp & in=out & dt1=dt2 & len1=len2)
    \/ (exists u,dt3,len3. in->RefSll_t{next:u,data_sbd:dt1} * lsls(u,dt3,len3,out,dt2,len2) & error & len1=len3+1);


checkentail lsls(X,d1,n1,Y,d2,n2) * Y->RefSll_t{next:Z,data_sbd:d3} * lsls(Z,d4,n3,U,d5,n4) & d1!=d2 & d2=d3 & n3=n4+2 & n2=n3+1 & error
    |- lsls(X,d1,n1,U,d5,n4) & error & error;
