
data RefDll_t {
    RefDll_t next;
    RefDll_t prev;
    unknown data_sbd;
};

pred sdll(E,P,dt1,F,L,dt2) := 
    (emp & E=F & P=L & dt1=dt2)
    \/ (exists u,dt3. E->RefDll_t{next:u,prev:P,data_sbd:dt1} * sdll(u,E,dt3,F,L,dt2) & error);


checkentail sdll(x1,x2,d1,y1,y2,d2) & error & x1=y2
    |- false;
