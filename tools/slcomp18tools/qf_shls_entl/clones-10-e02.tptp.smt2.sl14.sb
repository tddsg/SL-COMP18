
data RefSll_t {
    RefSll_t next;
};

pred ls(in,out) := 
    (emp & in=out)
    \/ (exists u. in->RefSll_t{next:u} * ls(u,out) & in!=out);


checkentail emp & nil=nil & nil!=x1 & nil!=x2 & x1!=x2 & nil!=x3 & nil!=x4 & x3!=x4 & nil!=x5 & nil!=x6 & x5!=x6 & nil!=x7 & nil!=x8 & x7!=x8 & nil!=x9 & nil!=x10 & x9!=x10 & nil!=x11 & nil!=x12 & x11!=x12 & nil!=x13 & nil!=x14 & x13!=x14 & nil!=x15 & nil!=x16 & x15!=x16 & nil!=x17 & nil!=x18 & x17!=x18 & nil!=x19 & nil!=x20 & x19!=x20
    |- emp;
