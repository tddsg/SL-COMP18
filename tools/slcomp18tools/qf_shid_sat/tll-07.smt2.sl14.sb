
data RefTll_t {
    RefTll_t lson;
    RefTll_t rson;
    RefTll_t next;
};

pred tll(r,ll,rl) := 
    (r->RefTll_t{lson:nil,rson:nil,next:rl} & r=ll)
    \/ (exists ls,rs,z. r->RefTll_t{lson:ls,rson:rs,next:nil} * tll(ls,ll,z) * tll(rs,z,rl));


checkentail tll(x0,x1,x2) * tll(x2,x3,x4) * tll(x4,x5,x6) * tll(x6,x7,x8) & x0=x1 & x2!=x3 & x4=x5 & x6!=x7 & x8=x0
    |- false;
